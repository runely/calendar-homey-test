{
  "name": "no.runely.calendar.test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "Rune Moskvil Lyng√•s <rune.moskvil.lyngaas@vtfk.no>",
  "license": "ISC",
  "dependencies": {
    "chalk": "4.1.0",
    "humanize-duration": "3.25.0",
    "moment-timezone": "0.5.32",
    "node-ical": "0.12.7",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "xo": "0.36.1"
  },
  "scripts": {
    "refresh": "rm -rf node_modules/ && rm package-lock.json && npm i",
    "calendars": "node from-file.js",
    "calendars-and-next-event": "node from-file.js --nextEvent",
    "calendars-and-todays-events": "node from-file.js --todaysEvents",
    "calendars-and-tomorrows-events": "node from-file.js --tomorrowsEvents",
    "test": "xo",
    "fix": "xo --fix"
  },
  "xo": {
    "space": 2,
    "semicolon": true,
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "object-curly-spacing": "off",
      "eol-last": "off",
      "capitalized-comments": "off",
      "no-negated-condition": "off",
      "radix": "warn"
    }
  }
}
